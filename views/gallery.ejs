<html>
<head>
	<title>Gif Gallery</title>
</head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/themes/smoothness/jquery-ui.css" />
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script>
<script src="./javascript/packery_pkgd.js"></script>
<style type="text/css">
	.img-item {
		width: 24%;
		height: auto;
		float: left;
	}


</style>
<script type="text/javascript">
	$( function() {
	  var $container = $('.js-packery').packery({
	  	itemSelector: ".img-item",
	  	gutter: 11
	    //columnWidth: 80,
	    //rowHeight: 80
	  });
	  // get item elements, jQuery-ify them
	  var $itemElems = $container.find('.img-item');
	  // make item elements draggable
	  $itemElems.draggable();
	  // bind Draggable events to Packery
	  $container.packery( 'bindUIDraggableEvents', $itemElems );
	});

</script>


<body>
	<h1>Hello</h1>
	<% if (typeof gif !== 'undefined') { %>
		<% for (var i=0;i<gif.length; i++) { %>
	<img src="<%= gif[i].images.fixed_width.url %>">
		<% } %>
	<% } %>
	<div id="container" class="js-packery" data-packery-options='{"itemSelector": ".img-item", "gutter": 11}'>
		<img class='img-item' src="http://media0.giphy.com/media/1EzbweBp6XRv2/200w.gif">
		<img class='img-item' src="http://media3.giphy.com/media/pYK2AuGaJnePm/200w.gif">
		<img class='img-item' src="http://media0.giphy.com/media/e5s9AhceLnmfe/200w.gif">
		<img class='img-item' src="http://media1.giphy.com/media/vctPkvEb98wNO/200w.gif">
		<img class='img-item' src="http://media1.giphy.com/media/hkC9qHkrrfdqE/200w.gif">
		<img class='img-item' src="http://media3.giphy.com/media/NFo6yazDq7q9y/200w.gif">
		<img class='img-item' src="http://media0.giphy.com/media/ZFlW5pAMdPjJm/200w.gif">
		<img class='img-item' src="http://media4.giphy.com/media/4PgaTslLWFw7m/200w.gif">
		<img class='img-item' src="http://media1.giphy.com/media/crJdvW1VXflug/200w.gif">
		<img class='img-item' src="http://media4.giphy.com/media/ctXeD2i0KmGru/200w.gif">

	</div>



</body>
</html>